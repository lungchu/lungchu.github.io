<category name="GuangGuang" colour="255">
<!-- 光光小車 -->
  <category name="ggMiniCar">
    <block type="ggMiniCarInit">
				<value name="L1">
					<block type="math_number">
						<field name="NUM">25</field>
					</block>
				</value>
				<value name="L2">
					<block type="math_number">
						<field name="NUM">26</field>
					</block>
				</value>
        <field name="LeftReverse">FALSE</field>
				<value name="R1">
					<block type="math_number">
						<field name="NUM">32</field>
					</block>
				</value>
				<value name="R2">
					<block type="math_number">
						<field name="NUM">33</field>
					</block>
				</value>
        <field name="RightReverse">FALSE</field>
    </block>
    <block type="ggMiniCarInitUS">
        <field name="SensorPos">fLeft</field>
				<value name="USTrigPin">
					<block type="math_number">
						<field name="NUM">12</field>
					</block>
				</value>
				<value name="USEchoPin">
					<block type="math_number">
						<field name="NUM">13</field>
					</block>
				</value>
    </block>
    <block type="ggMiniCarInitIR">
        <field name="SensorPos">fLeft</field>
				<value name="IrPin">
					<block type="math_number">
						<field name="NUM">35</field>
					</block>
				</value>
    </block>
    <block type="ggMiniCarLineAdjust">
				<value name="LineAdjust">
					<block type="math_number">
						<field name="NUM">0</field>
					</block>
				</value>
    </block>
    <block type="ggMiniCarSetSpeed">
				<value name="Speed">
					<block type="math_number">
						<field name="NUM">80</field>
					</block>
				</value>
    </block>
    <block type="ggMiniCarMove">
			<field name="MoveType">Forward</field>
			<field name="MyTurn">Low</field>
			<value name="ArcSpeed">
				<block type="math_number">
					<field name="NUM">20</field>
				</block>
			</value>
    </block>
    <block type="ggMiniCarUSSetUnit">
      <field name="USUnit">1</field>
    </block>
    <block type="ggMiniCarUSGetDistance">
      <field name="SensorPos">fLeft</field>
    </block>
    <block type="ggMiniCarIrDetectWhite">
      <field name="SensorPos">fLeft</field>
    </block>
<!-- ggMiniCar Servo -->
    <category name="ggMiniCarServo">
    <block type="ggMiniCar_servo_custom_attach">
			<value name="PIN">
				<block type="math_number">
					<field name="NUM">0</field>
				</block>
			</value>
			<value name="MAX">
				<block type="math_number">
					<field name="NUM">544</field>
				</block>
			</value>
			<value name="MIN">
				<block type="math_number">
					<field name="NUM">2400</field>
				</block>
			</value>
    </block>
		<block type="ggMiniCar_servo_write">
			<value name="PIN">
				<block type="math_number">
					<field name="NUM">0</field>
				</block>
			</value>
			<value name="ANGLE">
				<block type="math_number">
					<field name="NUM">0</field>
				</block>
			</value>
		</block>
		<block type="ggMiniCar_servo_writeus">
			<value name="PIN">
				<block type="math_number">
					<field name="NUM">0</field>
				</block>
			</value>
			<value name="ANGLE_US">
				<block type="math_number">
					<field name="NUM">1500</field>
				</block>
			</value>
		</block>
		<block type="ggMiniCar_servo_read">
  		<value name="PIN">
				<block type="math_number">
					<field name="NUM">0</field>
				</block>
			</value>
    </block>
  </category>
